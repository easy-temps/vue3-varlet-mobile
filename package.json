{
  "name": "vue3-varlet-mobile",
  "type": "module",
  "version": "0.9.0",
  "packageManager": "pnpm@9.0.6",
  "description": "Vue + Vite H5 Starter Template",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env MOCK_SERVER_PORT=8086 vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "release": "bumpp --commit --push --tag",
    "prepare": "husky"
  },
  "dependencies": {
    "@unhead/vue": "^1.9.5",
    "@varlet/touch-emulator": "^3.2.6",
    "@varlet/ui": "^3.2.6",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "echarts": "^5.5.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "resize-detector": "^0.3.0",
    "store": "^2.0.12",
    "vconsole": "^3.15.1",
    "vue": "^3.4.23",
    "vue-i18n": "^9.12.1",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.14.0",
    "@iconify-json/carbon": "^1.1.31",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/nprogress": "^0.2.3",
    "@types/store": "^2.0.5",
    "@unocss/eslint-plugin": "0.59.3",
    "@unocss/preset-rem-to-px": "0.59.3",
    "@varlet/import-resolver": "^3.2.6",
    "@varlet/preset-unocss": "^3.2.6",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.0",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "eslint": "npm:eslint-ts-patch@8.57.0-0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.57.0-0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "mockjs": "^1.1.0",
    "postcss-mobile-forever": "^4.1.2",
    "rollup": "^4.14.3",
    "terser": "^5.30.3",
    "typescript": "^5.4.5",
    "unocss": "0.59.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.8.6",
    "vite": "^5.2.9",
    "vite-plugin-mock-dev-server": "^1.5.0",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-sitemap": "^0.5.3",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.0.27",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "esbuild"
      ],
      "allowedVersions": {
        "rollup": "^4.x"
      }
    }
  }
}
